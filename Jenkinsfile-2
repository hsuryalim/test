pipeline {
	agent any

	options {
		timestamps()
	}

	environment {
		name1 = "Jeff"
		name2 = "John"
	}

	stages {
		stage('Hello World') {
			environment {
				name2 = "Michael"
				name3 = "Jamie"
			}
			steps {
				echo "name1 = ${name1}"
				echo "name2 = ${name2}"
				echo "name3 = ${name3}"
				sh "printenv"
			}
		}
		stage('Test') {
			steps {
				echo "test"
			}
		}
	}
}
