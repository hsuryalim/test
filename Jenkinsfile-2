pipeline {
	agent any

	options {
		timestamps()
	}

	stages {
		stage('Build') {
			steps {
				echo "build step.."
				//script {
				//	currentBuild.result = "UNSTABLE"
				//}
			}
		}
	}

	post {
		success {
			echo "post->success is called"
		}
		failure {
			echo "post->failure is called"
		}
		always {
			echo "post->always is called"
		}
		changed {
			echo "post->changed is called"
		}
	}
}
