pipeline {
	agent any
	
	options {
		timestamps()
	}

	environment {
		EXAMPLE_1 = credentials('example_1')
		EXAMPLE_2 = credentials('example_2')
	}
	
	stages {
		stage('Phase 1') {
			echo "example 1 ${EXAMPLE_1}"
			echo "example 1 user ${EXAMPLE_1_USR}"
			echo "example 1 password ${EXAMPLE_1_PSW}"
			echo "example 2 ${EXAMPLE_2}"
		}
	}
}
